<Window x:Class="AllTechnologyWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AllTechnologyWpf"
        mc:Ignorable="d"
        Title="Технологии Wpf C#" Height="650" Width="800" MinHeight="650" MinWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Image Source="/Imports/Logo.png" Grid.Row="0" Height="30" Width="50" HorizontalAlignment="Left" Margin="20,10,10,10" RenderOptions.BitmapScalingMode="Fant"></Image>
        <Button x:Name="Back" Content="Назад" Margin="10" HorizontalAlignment="Right" Click="Back_Click" Style="{StaticResource StandartButton}"></Button>
        <Frame x:Name="MyFrame" NavigationUIVisibility="Hidden" ContentRendered="MyFrame_ContentRendered" Grid.Row="1"></Frame>
    </Grid>
</Window>
